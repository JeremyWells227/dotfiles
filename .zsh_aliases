alias a='alias'
a vim='nvim'
a vim8='/usr/local/bin/vim'
a lr='ls --color=auto -altrH'
a l='ls --color=auto -altH'
a s='source ~/.zshrc'
a u='cd ..'
a u2='cd ../../'
a u3='cd ../../../'
a u4='cd ../../../../'
a u5='cd ../../../../../'
a i3config='nvim ~/.config/i3/config' 
a mordhau='steam steam://rungameid/629760 & disown && exit'
a bspconfig="nvim ~/.config/bspwm/bspwmrc"
a hk="nvim ~/.config/sxhkd/sxhkdrc"
a bspreload="~/.config/bspwm/bspwmrc"
a polyconfig="nvim ~/.config/polybar/config"
a lf='lfrun'
a pbcopy='xsel --clipboard --input'
a pbpaste='xsel --clipboard --output'
a ba="vim ~/.bash_aliases"
a za="vim ~/.zsh_aliases"
a ssh_agent_setup="eval $(ssh-agent) ssh-add ~/.ssh/id_ed25519"
a remove_mouse_accel='xinput set-prop "Logitech G502 HERO Gaming Mouse" "libinput Accel Profile Enabled" 0, 1'
a cdf='cd `fd . -t d | fzf`'
